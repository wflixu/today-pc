{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },
  "rules": {
    "no-namespace": [
      false,
      "allow-declarations"
    ],
    "interface-name": [false, "never-prefix"],
    "member-access": [false, "no-public"],
    "array-type": [true, "array"],
    "jsx-no-lambda":false,
    "curly":false,
    "object-literal-sort-keys":false,
    "ordered-imports": false
  },
  "ordered-imports": false
}